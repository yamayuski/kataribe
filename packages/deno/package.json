{
  "name": "@kataribe/deno",
  "version": "0.4.0",
  "description": "Deno implementation for kataribe - WebSocket, WebRTC, WebTransport",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yamayuski/kataribe.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "echo 'Deno package - skipping build as it uses native Deno runtime'",
    "test": "cd src/ws && deno test --no-check transport_test.ts"
  },
  "keywords": [
    "rpc",
    "websocket",
    "webrtc",
    "webtransport",
    "deno",
    "typescript"
  ],
  "dependencies": {
    "@kataribe/core": "workspace:*"
  },
  "devDependencies": {
    "@tsconfig/deno": "^1.0.11",
    "@kataribe/internal": "workspace:*"
  }
}
