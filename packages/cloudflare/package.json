{
  "name": "@kataribe/cloudflare",
  "version": "0.1.0",
  "description": "Cloudflare Workers implementation for kataribe - WebSocket with Durable Objects",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "npx rimraf dist",
    "build:types": "npx wrangler types && tsc --emitDeclarationOnly"
  },
  "keywords": [
    "rpc",
    "websocket",
    "cloudflare",
    "workers",
    "typescript"
  ],
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@kataribe/core": "workspace:*"
  },
  "devDependencies": {
    "wrangler": "^4.39.0"
  }
}
