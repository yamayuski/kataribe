{
  "name": "@kataribe/cloudflare",
  "version": "0.3.0",
  "description": "Cloudflare Workers implementation for kataribe - WebSocket with Durable Objects",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yamayuski/kataribe.git"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npx wrangler types src/worker-configuration.d.ts && tsc --emitDeclarationOnly",
    "test": "vitest"
  },
  "keywords": [
    "rpc",
    "websocket",
    "cloudflare",
    "workers",
    "typescript"
  ],
  "dependencies": {
    "@kataribe/core": "workspace:*"
  },
  "devDependencies": {
    "wrangler": "^4.37.0",
    "vitest": "catalog:",
    "@kataribe/internal": "workspace:*"
  }
}
