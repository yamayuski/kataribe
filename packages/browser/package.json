{
  "name": "@kataribe/browser",
  "version": "0.3.0",
  "description": "Browser implementation for kataribe - WebSocket, WebRTC, WebTransport",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yamayuski/kataribe.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest"
  },
  "keywords": [
    "rpc",
    "websocket",
    "webrtc",
    "webtransport",
    "browser",
    "typescript"
  ],
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@kataribe/core": "workspace:*"
  },
  "devDependencies": {
    "vitest": "catalog:",
    "jsdom": "catalog:",
    "@kataribe/internal": "workspace:*"
  }
}
