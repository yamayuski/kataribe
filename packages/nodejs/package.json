{
  "name": "@kataribe/nodejs",
  "version": "0.1.0",
  "description": "Node.js implementation for kataribe - WebSocket, WebRTC",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "keywords": [
    "rpc",
    "websocket",
    "webrtc",
    "nodejs",
    "typescript"
  ],
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@kataribe/core": "workspace:*",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "catalog:",
    "@types/ws": "catalog:"
  }
}
