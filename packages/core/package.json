{
  "name": "@kataribe/core",
  "version": "0.1.0",
  "description": "Core types and utilities for kataribe - bidirectional RPC + events",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "keywords": [
    "rpc",
    "typescript",
    "types",
    "envelope",
    "core"
  ],
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@types/node": "catalog:"
  }
}
